---
interface Props {
    title?: string;
}

const { title = "Inled - We make a better world" } = Astro.props;
---

<!DOCTYPE html>
<html lang="es">
    <head>
        <meta charset="UTF-8" />
        <meta name="viewport" content="width=device-width" />
        <link rel="icon" type="image/svg+xml" href="/favicon.svg" />
        <link rel="stylesheet" href="/css/style.css">
        <title>{title}</title>
    </head>
    <body>
        <slot />
    </body>
</html>
